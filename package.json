{
  "name": "eleventy-plugin-embed-cloudinary",
  "description": "Eleventy plugin to automatically embed images hosted on Cloudinary.",
  "license": "MIT",
  "version": "1.0.1",
  "author": {
    "name": "Giacomo Debidda",
    "email": "giacomo@giacomodebidda.com",
    "url": "https://giacomodebidda.com/"
  },
  "homepage": "https://github.com/jackdbd/eleventy-plugin-embed-cloudinary#readme",
  "keywords": [
    "11ty",
    "cloudinary",
    "eleventy",
    "eleventy-plugin",
    "image"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackdbd/eleventy-plugin-embed-cloudinary"
  },
  "bugs": {
    "url": "https://github.com/jackdbd/eleventy-plugin-embed-cloudinary/issues"
  },
  "main": "dist/.eleventy.js",
  "module": "dist/eleventy-plugin-embed-cloudinary.esm.js",
  "typings": "dist/.eleventy.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsdx build --entry ./src/.eleventy.ts --target node && mv dist/index.js dist/.eleventy.js",
    "clean": "rm -rf dist",
    "doctoc": "doctoc --github README.md --title '<details><summary>Table of Contents</summary>'",
    "bundlewatch": "bundlewatch --config ./config/bundlewatch.js",
    "docs:ae": "api-extractor run --config ./api-extractor.json --local --verbose",
    "docs:ad": "api-documenter markdown --input-folder .ae/doc --output-folder docs-ae",
    "docs:typedoc": "typedoc src/api-extractor.ts --out docs --theme default",
    "docs": "run-s 'docs:ae' 'docs:ad' 'docs:typedoc' --print-label",
    "lint": "tsdx lint",
    "nuke": "npm run clean && rm -rf node_modules package-lock.json",
    "prepare": "husky install",
    "prepack": "npm run build && ./scripts/clean-package-json",
    "postpack": "./scripts/restore-package-json && ./scripts/create-github-release",
    "precommit": "run-s 'lint' 'doctoc'",
    "prerelease": "echo PRELEASE",
    "release": "xyz --branch main --publish-command 'npm publish' --script './scripts/update-changelog'",
    "release:patch": "npm run release -- --increment patch",
    "release:minor": "npm run release -- --increment minor",
    "release:major": "npm run release -- --increment major",
    "postrelease": "echo 'See package on npm' && chromium-browser https://www.npmjs.com/package/eleventy-plugin-embed-cloudinary",
    "start": "tsdx watch --entry ./src/.eleventy.ts --target node --verbose",
    "test": "tsdx test --detectOpenHandles --coverage=true --verbose",
    "test:config": "tsdx test --showConfig"
  },
  "dependencies": {},
  "peerDependencies": {
    "@11ty/eleventy": ">=1.0.0",
    "@11ty/eleventy-cache-assets": ">=2.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "7.17.17",
    "@microsoft/api-extractor": "7.25.0",
    "auto-changelog": "^2.2.1",
    "bundlewatch": "0.3.3",
    "doctoc": "2.2.0",
    "husky": "8.0.1",
    "npm-run-all": "4.1.5",
    "tsdx": "0.14.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "4.7.3",
    "xyz": "4.0.0"
  }
}
